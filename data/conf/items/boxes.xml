<?xml version="1.0" encoding="UTF-8"?>
<items xmlns="stendhal" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="stendhal ../items.xsd ">

	<item name="prezent">
		<type class="box" subclass="present" tileid="-1"/>
		<description>Widzisz prezent. Otwórz go.</description>
		<implementation class-name="games.stendhal.server.entity.item.Present"/>
		<attributes>
		</attributes>
		<weight value="0.01"/>
		<value value="1"/>
		<equipable>
			<slot name="ground"/>
			<slot name="content"/>
			<slot name="bag"/>
			<slot name="trade"/>
		</equipable>
	</item>

	<item name="koszyk">
		<type class="box" subclass="basket" tileid="-1"/>
		<description>Widzisz koszyk. Spójrz do niego.</description>
		<implementation class-name="games.stendhal.server.entity.item.Basket"/>
		<attributes>
		</attributes>
		<weight value="0.01"/>
		<value value="1"/>
		<equipable>
			<slot name="ground"/>
			<slot name="content"/>
			<slot name="bag"/>
			<slot name="trade"/>
		</equipable>
	</item>

	<item name="skarpeta">
		<type class="box" subclass="stocking" tileid="-1"/>
		<description>To skarpeta. Spójrz do środka.</description>
		<implementation class-name="games.stendhal.server.entity.item.Stocking"/>
		<attributes>
		</attributes>
		<weight value="0.01"/>
		<value value="100"/>
		<equipable>
			<slot name="ground"/>
			<slot name="content"/>
			<slot name="bag"/>
			<slot name="trade"/>
		</equipable>
	</item>

	<item name="skrzynka">
		<type class="box" subclass="skrzynka" tileid="-1"/>
		<description>Widzisz skrzynkę. Otwórz ją.</description>
		<implementation class-name="games.stendhal.server.entity.item.Present"/>
		<attributes/>
		<weight value="0.01"/>
		<value value="1"/>
		<equipable>
			<slot name="ground"/>
			<slot name="content"/>
			<slot name="bag"/>
			<slot name="trade"/>
		</equipable>
	</item>
</items>