<?xml version="1.0" encoding="UTF-8"?>
<items xmlns="stendhal" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="stendhal ../items.xsd ">

  <item name="pierścień z mithrilu">
    <type class="animation_ring" subclass="mithril_ring" tileid="-1"/>
    <description>Oto pierścień z mithrilu. Został wykonany z bardzo wysoko-jakościowego materiału.</description>
    <implementation class-name="games.stendhal.server.entity.item.Item"/>
    <attributes>
      <atk value="4"/>
      <def value="5"/>
    </attributes>
    <weight value="0.1"/>
    <value value="323032"/>
    <susceptibility type="dark" value="1.11"/>
    <susceptibility type="light" value="0.83"/>
    <equipable>
      <slot name="ground"/>
      <slot name="content"/>
      <slot name="bag"/>
      <slot name="trade"/>
      <slot name="finger"/>
      <slot name="fingerb"/>
      <slot name="keyring"/>
    </equipable>
  </item>

